{
  "name": "megafon-aero-test",
  "version": "1.0.0",
  "license": "ISC",
  "private": true,
  "browserslist": [
    "defaults",
    "last 2 versions",
    "not IE 11",
    "> 0.5%"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.aeroidea.ru/aero/nextjs-starter-kit.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm-run-all --parallel start server",
    "lint": "tsc && next lint --fix",
    "start": "NODE_ENV=production next start",
    "analyze": "ANALYZE=true next build",
    "favicons": "real-favicon generate src/assets/meta/meta-description.json src/assets/meta/meta-data.json ./public/meta && cpy public/meta/favicon.ico public",
    "prepare": "husky install",
    "component": "node create-component.js",
    "server": "node ./mock/server.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "html-react-parser": "^1.2.6",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "next": "^13.1.6",
    "next-cookies": "^2.0.3",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-input-mask": "^2.0.4",
    "react-perfect-scrollbar": "^1.5.8",
    "sass": "^1.35.2",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@svgr/webpack": "^5.5.0",
    "@types/node": "^16.0.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-input-mask": "^3.0.2",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "cli-real-favicon": "^0.0.8",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.30.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "jira-prepare-commit-msg": "^1.6.1",
    "npm-run-all": "^4.1.5",
    "pinst": "^2.1.6",
    "sass-resources-loader": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "$J $M",
    "jiraTicketPattern": "([A-Z]+-\\d+)",
    "commentChar": "#",
    "isConventionalCommit": false,
    "allowEmptyCommitMessage": false
  }
}
